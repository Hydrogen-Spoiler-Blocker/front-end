{
  "name": "webpackspoilerblocker2",
  "options_page": "options.html",
  "version": "1",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage",
    "http://domain/getjson*",
    "webRequest",
    "webRequestBlocking",
    "https://*/*",
    "http://*/*"
  ],
  "web_accessible_resources": [
    "https/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  
}
