{
    "name": "Spoiler Blocker",
    "description": "Blocks spoilers",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons":{
        "16": "icons/SB-icon16.png",
        "32": "icons/SB-icon32.png",
        "48": "icons/SB-icon48.png"
        
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}
